<apex:page controller="RadioControllor">
    <apex:form>
        <apex:pageBlock >
            <apex:pageBlockButtons >
                <apex:commandButton action="{!Save}" value="Save" />
                <apex:commandButton action="{!Clear}" value="Clear" />
                <apex:commandButton action="{!Cancil}" value="Cancil" />
            </apex:pageBlockButtons>
            <apex:selectRadio value="{!billContact}">
                <apex:selectOptions value="{!items}"/>
            </apex:selectRadio>
        </apex:pageBlock>
    </apex:form>

    
</apex:page>